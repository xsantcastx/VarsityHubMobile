{
  "name": "varsityhubmobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "start:ci": "set CI=1&& expo start --port 19001",
    "web:ci": "set CI=1&& expo start --web --port 9500",
    "android:ci": "set CI=1&& expo start --android --port 19002",
    "postinstall": "node ./tools/ensure-simple-swizzle.js && node ./tools/ensure-debug-index.js && node ./tools/patch-router-sitemap.js && node ./tools/ensure-is-arrayish.js && node ./tools/patch-logbox-button.js",
    "fix:deps": "node ./tools/ensure-simple-swizzle.js && node ./tools/ensure-debug-index.js && node ./tools/ensure-is-arrayish.js",
    "lint": "expo lint",
    "server:install": "cd server && npm install",
    "server:dev": "cd server && npm run dev",
    "server:db:migrate": "cd server && npx prisma migrate dev --name init",
    "server:db:seed": "cd server && npm run seed",
    "server:db:studio": "cd server && npx prisma studio"
  },
  "dependencies": {
    "@base44/sdk": "^0.7.0",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/datetimepicker": "^8.4.5",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "date-fns": "^4.1.0",
    "expo": "~53.0.22",
    "expo-av": "~15.1.7",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-document-picker": "~13.1.6",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-image-manipulator": "~13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-media-library": "~16.0.4",
    "expo-router": "~5.1.5",
    "expo-secure-store": "~13.0.2",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-video": "~2.2.2",
    "expo-web-browser": "~14.2.0",
    "lottie-ios": "^4.5.1",
    "lottie-react-native": "7.2.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-calendars": "^1.1313.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-view-shot": "4.0.3",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "expo-auth-session": "~6.2.1"
  },
  "devDependencies": {
    "@react-native-community/cli": "^20.0.2",
    "@typescript-eslint/parser": "^8.44.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint-plugin-react-native": "^4.0.0"
  },
  "private": true,
  "prisma": {
    "schema": "server/prisma/schema.prisma"
  }
}
