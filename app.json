{
  "expo": {
    "name": "VarsityHubMobile",
    "slug": "VarsityHubMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "varsityhubmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "locales": {
      "en": "./locales/en.json"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.anonymous.VarsityHubMobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "usesCleartextTraffic": true,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.VarsityHubMobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to attach images to posts.",
          "cameraPermission": "Allow $(PRODUCT_NAME) to use the camera for capturing photos and videos.",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access the microphone for recording videos."
        }
      ],
      "expo-video",
      "expo-web-browser",
      "expo-secure-store",
      "expo-font",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "dd17922e-328b-42af-a083-95eab643113a"
      }
    }
  }
}
